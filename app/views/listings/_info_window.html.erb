<div class="card bg-light" style="width: 10rem; border-radius: 16px;">
    <%= link_to listing_path(listing.id), class:'text-decoration-none' do %>
  <div class="card-body">
    <h5 class="card-title"><%= listing.developer %></h5>
    <h6 class="card-subtitle mb-2 text-muted"><%= listing.price %> $</h6>
  </div>
    <% end %>
</div>
